        "args": [
          "+nightly",
          "build",
          "--bin=day18",
          "--package=day18"
        ],
        "filter": {
          "name": "day18",
          "kind": "bin"
        }
      },
      "args": ["./input.txt"],
      "cwd": "${workspaceFolder}"
    },
    {
